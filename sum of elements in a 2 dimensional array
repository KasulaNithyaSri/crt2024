SOURCE CODE:

#include <stdio.h>

int main() {
    int m,n;
    scanf("%d %d",&m,&n);
    int a[m][n],sum=0;
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            scanf("%d",&a[i][j]);
           
        }
         printf("\n");
    }
    printf("matrix=");
    for(int i=0;i<m;i++){
        for(int j=0;j<n;j++){
            printf("%d ",a[i][j]);
            sum=sum+a[i][j];
        }
        printf("\n");
    }
   printf("Sum=%d",sum);
    return 0;
}

OUTPUT:

